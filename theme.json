{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"version": 2,
	"patterns": [],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": ["#444444", "#ffffff"],
					"slug": "duotone-1",
					"name": "Black and white"
				}
			],
			"gradients": [
				{
					"slug": "gradient",
					"gradient": "linear-gradient(to bottom, #f9fafb 0%, #eb7102 100%)",
					"name": "Vertical soft rust to white"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#f9fafb",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#444444",
					"name": "Contrast / Two",
					"slug": "contrast-2"
				},
				{
					"color": "#011935",
					"name": "Contrast / Three",
					"slug": "contrast-3"
				},
				{
					"color": "#eb7102",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#3c385a",
					"name": "Accent / Two",
					"slug": "accent-2"
				},
				{
					"color": "#00a590",
					"name": "Accent / Three",
					"slug": "accent-3"
				}
			]
		},
		"layout": {
			"contentSize": "805px",
			"wideSize": "1230px"
		},
		"spacing": {
			"spacingScale": {
				"steps": 7
			},
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "PT Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/pt-sans/pt-sans.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "PT Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/pt-sans/pt-sans-bold.woff2"
							]
						}
					],
					"fontFamily": "\"PT Sans\", sans-serif",
					"name": "PT Sans",
					"slug": "body"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "PT Serif",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/pt-serif/pt-serif.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "PT Serif",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/pt-serif/pt-serif-bold.woff2"
							]
						}
					],
					"fontFamily": "\"PT Serif\", serif",
					"name": "PT Serif",
					"slug": "heading"
				},
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, avenir next, avenir, segoe ui, helvetica neue, helvetica, Cantarell, Ubuntu, roboto, noto, arial, sans-serif",
