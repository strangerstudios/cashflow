				{
					"fluid": {
						"min": "1.688rem",
						"max": "1.875rem"
					},
					"name": "Extra Extra Large",
					"size": "1.875rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"min": "1.875rem",
						"max": "2.25rem"
					},
					"name": "Extra Extra Extra Large",
					"size": "2.25rem",
					"slug": "xxx-large"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "2.625rem"
					},
					"name": "Extra Extra Extra Extra Large",
					"size": "2.625rem",
					"slug": "xxxx-large"
				},
				{
					"fluid": {
						"min": "2.625rem",
						"max": "3.375rem"
