{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"version": 2,
	"patterns": [],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": ["#444444", "#ffffff"],
					"slug": "duotone-1",
					"name": "Black and white"
				}
			],
			"gradients": [
				{
					"slug": "gradient",
					"gradient": "linear-gradient(to bottom, #f9fafb 0%, #eb7102 100%)",
					"name": "Vertical soft rust to white"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#f9fafb",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#444444",
					"name": "Contrast / Two",
					"slug": "contrast-2"
				},
				{
					"color": "#011935",
					"name": "Contrast / Three",
					"slug": "contrast-3"
				},
				{
					"color": "#eb7102",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#3c385a",
					"name": "Accent / Two",
					"slug": "accent-2"
				},
				{
					"color": "#00a590",
					"name": "Accent / Three",
					"slug": "accent-3"
				}
			]
		},
		"layout": {
			"contentSize": "805px",
			"wideSize": "1230px"
		},
		"spacing": {
			"spacingScale": {
				"steps": 7
			},
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
			"fontSizes": [],
			"writingMode": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {},
		"color": {			
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast-3)"
		},
		"css": "",
		"elements": {
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--accent)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--accent-2)"
				}
			}
		},
		"shadow":{
		},
		"spacing": {
			"blockGap": "1.2rem"
		},
		"typography": {
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.7"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"customTemplates": []
}
