{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"version": 2,
	"patterns": [],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": ["#222222", "#ffffff"],
					"slug": "duotone-1",
					"name": "Black and white"
				}
			],
			"gradients": [
				{
					"slug": "gradient",
					"gradient": "linear-gradient(to bottom, #f9fafb 0%, #eb7102 100%)",
					"name": "Vertical soft rust to white"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#f5f8fa",
					"name": "Base / Two",
					"slug": "base-2"
				},
				{
					"color": "#222222",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#011718",
					"name": "Contrast / Two",
					"slug": "contrast-2"
				},
				{
					"color": "#034748",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#008473",
					"name": "Accent / Two",
					"slug": "accent-2"
				}
			]
		},
		"layout": {
			"contentSize": "805px",
			"wideSize": "1230px"
		},
		"spacing": {
			"spacingScale": {
				"steps": 7
			},
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Figtree",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/figtree/figtree.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Figtree",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/figtree/figtree-bold.woff2"
							]
						}
					],
					"fontFamily": "\"Figtree\", sans-serif",
					"name": "Figtree",
					"slug": "heading"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/inter/inter.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/inter/inter-bold.woff2"
							]
						}
					],
					"fontFamily": "\"Inter\", serif",
					"name": "Inter",
					"slug": "body"
				},
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, avenir next, avenir, segoe ui, helvetica neue, helvetica, Cantarell, Ubuntu, roboto, noto, arial, sans-serif",
					"name": "System Sans-serif",
					"slug": "system-sans-serif"
				},
				{
					"fontFamily": "Iowan Old Style, Apple Garamond, Baskerville, Times New Roman, Droid Serif, Times, Source Serif Pro, serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol",
					"name": "System Serif",
					"slug": "system-Serif"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.9rem",
					"slug": "small"
				},
				{
					"fluid": false,
					"name": "Medium",
					"size": "1.1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.375rem",
						"max": "1.7rem"
					},
					"name": "Large",
					"size": "1.7rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "2.625rem"
					},
					"name": "Extra Large",
					"size": "2.625rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "3.188rem",
						"max": "4.5rem"
					},
					"name": "Extra Extra Large",
					"size": "4.5rem",
					"slug": "xx-large"
				}
			],
			"writingMode": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "700"
				}
			}
		},
		"color": {			
			"background": "var(--wp--preset--color--base-2)",
			"text": "var(--wp--preset--color--contrast-2)"
		},
		"css": "",
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontWeight": "700",
					"lineHeight": "1.5"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "1.15"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontWeight": "700",
					"lineHeight": "1.3"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--accent-2)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--accent)"
				}
			}
		},
		"shadow":{
			"defaultPresets": true
		},
		"spacing": {
			"blockGap": "1.4rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.8"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"customTemplates": []
}
