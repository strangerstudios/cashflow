{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"version": 2,
	"patterns": [],
	"settings": {
		"appearanceTools": true,
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": ["#222222", "#ffffff"],
					"slug": "duotone-1",
					"name": "Black and white"
				}
			],
			"gradients": [
				{
					"slug": "gradient",
					"gradient": "linear-gradient(to bottom, #f9fafb 0%, #eb7102 100%)",
					"name": "Vertical soft rust to white"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#f5f8fa",
					"name": "Base / Two",
					"slug": "base-2"
				},
				{
					"color": "#222222",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#011718",
					"name": "Contrast / Two",
					"slug": "contrast-2"
				},
				{
					"color": "#dcdcdc",
					"name": "Contrast / Three",
					"slug": "contrast-3"
				},
				{
					"color": "#aaaaaa",
					"name": "Contrast / Four",
					"slug": "contrast-4"
				},
				{
					"color": "#034748",
					"name": "Accent",
					"slug": "accent"
				},
				{
					"color": "#008473",
					"name": "Accent / Two",
					"slug": "accent-2"
				},
				{
					"color": "#ffcc00",
					"name": "Accent / Three",
					"slug": "accent-3"
				}
			]
		},
		"custom": {
			"borderRadius": {
				"default": "6px",
				"circle": "50%"
			},
			"contextualMessage": {
				"default": {
					"backgroundColor": "#EBF5FF",
					"color": "#1E429F"
				},
				"success": {
					"backgroundColor": "#F3FAF7",
					"color": "#03543F"
				},
				"alert": {
					"backgroundColor": "#FDFDEA",
					"color": "#723B13"
				},
				"error": {
					"backgroundColor": "#FDF2F2",
					"color": "#9B1C1C"
				}
			},
			"formInput": {
				"color": "var(--wp--preset--color--contrast)",
				"background": "var(--wp--preset--color--base)",
				"borderColor": "var(--wp--preset--color--contrast-3)",
				"borderRadius": "var(--wp--preset--border-radius)"
			},
			"shadow": {
				"lift": "2px 6px 12px rgba( 0, 0, 0, 0.08 )"
			}
		},
		"layout": {
			"contentSize": "805px",
			"wideSize": "1230px"
		},
		"shadow": {
			"presets": [
				{
					"name": "Lift",
					"slug": "lift",
					"shadow": "var(--wp--custom-shadow--lift)"
				}
			]
		},
		"spacing": {
			"spacingScale": {
				"steps": 7,
				"mediumStep": 3,
				"unit": "rem"
			},
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Figtree",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/figtree/figtree.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Figtree",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/figtree/figtree-bold.woff2"
							]
						}
					],
					"fontFamily": "\"Figtree\", sans-serif",
					"name": "Figtree",
					"slug": "heading"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/inter/inter.woff2"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/inter/inter-bold.woff2"
							]
						}
					],
					"fontFamily": "\"Inter\", serif",
					"name": "Inter",
					"slug": "body"
				},
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, avenir next, avenir, segoe ui, helvetica neue, helvetica, Cantarell, Ubuntu, roboto, noto, arial, sans-serif",
					"name": "System Sans-serif",
					"slug": "system-sans-serif"
				},
				{
					"fontFamily": "Iowan Old Style, Apple Garamond, Baskerville, Times New Roman, Droid Serif, Times, Source Serif Pro, serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol",
					"name": "System Serif",
					"slug": "system-Serif"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.9rem",
					"slug": "small"
				},
				{
					"fluid": false,
					"name": "Medium",
					"size": "1.1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.375rem",
						"max": "1.7rem"
					},
					"name": "Large",
					"size": "1.7rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "2.625rem"
					},
					"name": "Extra Large",
					"size": "2.625rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "3.188rem",
						"max": "4.5rem"
					},
					"name": "Extra Extra Large",
					"size": "4.5rem",
					"slug": "xx-large"
				}
			],
			"writingMode": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/avatar": {
				"border": {
					"radius": "var(--wp--custom--border-radius--circle)"
				}
			},
			"core/button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--default)"
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "0.6rem"
				}
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--contrast-3)",
					"text": "var(--wp--preset--color--contrast)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)",
						"top": "var(--wp--preset--spacing--30)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"lineHeight": "1.6"
				}
			},
			"core/gallery": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--30)"
					}
				}
			},
			"core/image": {},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)"
				}
			},
			"core/pullquote": {
				"border": {
					"radius": "var(--wp--custom--border-radius--default)"
				}
			},
			"core/post-author": {},
			"core/preformatted": {
				"color": {
					"background": "var(--wp--preset--color--contrast-3)",
					"text": "var(--wp--preset--color--contrast)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)",
						"top": "var(--wp--preset--spacing--30)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"lineHeight": "1.6"
				}
			},
			"core/quote": {
				"elements": {
					"cite": {
						"typography": {
							"fontStyle": "normal"
						}
					}
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--30)/2",
						"right": "var(--wp--preset--spacing--30)/2",
						"bottom": "var(--wp--preset--spacing--30)/2",
						"left": "var(--wp--preset--spacing--30)/2"
					},
					"padding": {
						"top": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)",
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--30)"
					}
				}
			},
			"core/separator": {
				"border": {
					"color": "var(--wp--preset--color--contrast-4)",
					"style": "solid",
					"width": "0 0 1px 0"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast-4)"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "700"
				}
			}
		},
		"color": {			
			"background": "var(--wp--preset--color--base-2)",
			"text": "var(--wp--preset--color--contrast-2)"
		},
		"css": "",
		"elements": {
			"button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--default)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontWeight": "700",
					"lineHeight": "1.5"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "1.15"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontWeight": "700",
					"lineHeight": "1.3"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--accent-2)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--accent)"
				}
			}
		},
		"shadow":{
			"defaultPresets": true
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--40)",
			"padding": {
				"left": "var(--wp--preset--spacing--30)",
				"right": "var(--wp--preset--spacing--30)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.7"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"customTemplates": [
		{
			"name": "page-blank",
			"postTypes": ["page"],
			"title": "Page: Blank"
		},
		{
			"name": "page-no-title",
			"postTypes": ["page"],
			"title": "Page: No Title"
		},
		{
			"name": "page-wide",
			"postTypes": ["page"],
			"title": "Page: Wide"
		},
		{
			"name": "page-with-sidebar",
			"postTypes": ["page"],
			"title": "Page: With Sidebar"
		}
	]
}
